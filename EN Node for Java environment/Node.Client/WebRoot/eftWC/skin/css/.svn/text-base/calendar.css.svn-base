
.divCalTitleFont { font: bold 8pt Arial, Helvetica, sans-serif; color: #000000; }
.divCalDOWFont { font:bold 8pt Arial, Helvetica, sans-serif;	color: #FFFFFF; }
.divCalTodayFont { font: bold 8pt Arial, Helvetica, sans-serif; }
.divCalOtherDateFont { font: bold 7pt Verdana, Arial, Helvetica, sans-serif; }
.divCalDateFont { font:bold 7pt Verdana, Arial, Helvetica, sans-serif; }

.divCalCrossBG {
	background-attachment: fixed;
	background-image: url(../img/gen/Cal_X.gif);
	background-repeat: no-repeat;
	background-position: center center;
}
.divCalWeekdayBG {
	background-attachment: fixed;
	background-image: url(../img/gen/Cal_WeekdayBG.gif);
	background-repeat: no-repeat;
	background-position: center center;
}
.divCalWeekendBG {
	background-attachment: fixed;
	background-image: url(../img/gen/Cal_WeekendBG.gif);
	background-repeat: no-repeat;
	background-position: center center;
}
.divCalBG {
	background-attachment: fixed;
	background-image: url(../img/bg/paperBG_03.jpg);
	background-repeat: repeat;
}
.divCalTodayBG {
	background-attachment: fixed;
	background-image: url(../img/gen/Cal_OrangeBG.gif);
	background-repeat: no-repeat;
}
.divCalSelectDateBG {
	background-attachment: fixed;
	background-image: url(../img/gen/Cal_Circle.gif);
	background-repeat: no-repeat;
}

table#divCalMonthYear { margin:0; padding:0; border:0; }
table#divCalMonthYear td { margin:0; padding:0 3px; border:0; text-align:center; }

table#divCalDate { margin:0; padding:0; border:0px none;}
table#divCalDate td { margin:0; color:#999999; padding:0 2px; border-left:1px solid #FFFFFF; border-top:1px solid #FFFFFF; border-right:1px solid #999999;
	border-bottom:1px solid #999999; text-align:center; vertical-align:middle; }
table#divCalDate a { color:#000099; text-decoration:none; display:block; }
table#divCalDate a:hover { color:#FFFFFF; background-color:#000099; }

table#divCalTodayLine { margin:0; padding:0; border:0; }
table#divCalTodayLine td { margin:0; padding:0; border:0; text-align:center; }
table#divCalTodayLine a.today { color:#000099; text-decoration:none; padding:0 3px; }
table#divCalTodayLine a.today:hover { color:#FFFFFF; background-color:#000099; }

a.offset:hover { position:relative; top:1px; left:1px; }
