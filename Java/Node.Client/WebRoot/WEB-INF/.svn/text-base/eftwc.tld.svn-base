	<short-name>eftwc</short-name>
	<description>enfoTech WebControl Tag Library</description>
	<!--LinkSrc-->
	<tag>
		<name>LinkSrc</name>
		<tag-class>com.enfotech.component.UI.webControl.tags.LinkSrc</tag-class>
		<body-content>EMPTY</body-content>
		<description>link source file for all web control</description>
		<attribute>
			<name>linkBase</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Relative or absolute web control base.</description>
		</attribute>
	</tag>
	<!-- AquaButton -->
	<tag>
		<name>AquaButton</name>
		<tag-class>com.enfotech.component.UI.webControl.tags.AquaButton</tag-class>
		<tei-class>com.enfotech.component.UI.webControl.tags.AquaButtonInfo</tei-class>
		<body-content>EMPTY</body-content>
		<description>Aqua style button</description>
		<attribute>
			<name>linkBase</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Relative or absolute web control base.</description>
		</attribute>
		<attribute>
			<name>buttonText</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Text of button.</description>
		</attribute>
		<attribute>
			<name>buttonImage</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Image of button.</description>
		</attribute>
		<attribute>
			<name>textCSS</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Text style CSS.</description>
		</attribute>
		<attribute>
			<name>buttonStyle</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>[aquaBlue, aquaRed, aquaYellow, aquaGreen, aquaSilver and off].  Default is [aquaBlue].
	    </description>
		</attribute>
		<attribute>
			<name>onClickScript</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Javascript function when you click the button.</description>
		</attribute>
		<attribute>
			<name>fadeSpeed</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Set the speed of button fading effect.</description>
		</attribute>
		<attribute>
			<name>fadeLevel</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Set the speed of button fading level.</description>
		</attribute>
		<attribute>
			<name>fadeEffect</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Turn on/off fade effect. Default is off [false].</description>
		</attribute>
	</tag>
	<!-- FlushTag -->
	<tag>
		<name>FlushTag</name>
		<tag-class>com.enfotech.component.UI.webControl.tags.FlushTag</tag-class>
		<body-content>EMPTY</body-content>
		<description>Flush the jsp page</description>
	</tag>
	<!-- MessageLabel -->
	<tag>
		<name>MessageLabel</name>
		<tag-class>com.enfotech.component.UI.webControl.tags.MessageLabel</tag-class>
		<tei-class>com.enfotech.component.UI.webControl.tags.MessageLabelInfo</tei-class>
		<body-content>EMPTY</body-content>
		<description>Show message and icon label</description>
		<attribute>
			<name>object</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>MessageLabelBean object</description>
		</attribute>
		<attribute>
			<name>id</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>ID of tag</description>
		</attribute>
		<attribute>
			<name>linkBase</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Relative or absolute web control base.</description>
		</attribute>
		<attribute>
			<name>msgText</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Message you want to show in the page.</description>
		</attribute>
		<attribute>
			<name>msgIcon</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Message icon image:[OK, ERROR, WARNING, UNKNOWN and INFO]. Default is no icon.</description>
		</attribute>
		<attribute>
			<name>msgCSS</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Cascade style sheet class of the message font.</description>
		</attribute>
		<attribute>
			<name>msgCustomImage</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Customer images for message icon, full path needed.</description>
		</attribute>
		<attribute>
			<name>msgBreak</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Apply line break(br) before and/or after the message.</description>
		</attribute>
	</tag>
	<!-- HighlightTextBox -->
	<tag>
		<name>HighlightTextBox</name>
		<tag-class>com.enfotech.component.UI.webControl.tags.HighlightTextBox</tag-class>
		<body-content>EMPTY</body-content>
		<description>Textbox/textarea with onfocus change</description>
		<attribute>
			<name>object</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>HighlightTextBoxBean object</description>
		</attribute>
		<attribute>
			<name>id</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Field id and name of the textbox</description>
		</attribute>
		<attribute>
			<name>linkBase</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Relative or absolute web control base.</description>
		</attribute>
		<attribute>
			<name>type</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>It can be [text or password], default is text.</description>
		</attribute>
		<attribute>
			<name>maxLength</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Maxmium length of textbox. It will not be applied when it's a multi-line textarea</description>
		</attribute>
		<attribute>
			<name>size</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Size (width) of the textbox</description>
		</attribute>
		<attribute>
			<name>rows</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Rows of multi-line textarea</description>
		</attribute>
		<attribute>
			<name>text</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Value of the textbox</description>
		</attribute>
		<attribute>
			<name>script</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Javascript of the textbox</description>
		</attribute>
		<attribute>
			<name>disabled</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Disable the textbox, [true of false]</description>
		</attribute>
		<attribute>
			<name>readonly</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Make the textbox read only, [true of false]</description>
		</attribute>
		<attribute>
			<name>errorMsg</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Show error message on top of the textbox</description>
		</attribute>
		<attribute>
			<name>errorMsgCSS</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>font style class of error message</description>
		</attribute>
		<attribute>
			<name>errorMsgStyle</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>error message show-up style, [inline or pop]</description>
		</attribute>
		<attribute>
			<name>popMsgWidth</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>width of pop up error message, default is 180</description>
		</attribute>
		<attribute>
			<name>popMsgOffsetX</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>offsetX of pop up error message, default is 15</description>
		</attribute>
		<attribute>
			<name>popMsgOffsetY</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>offsetX of pop up error message, default is 0</description>
		</attribute>
	</tag>
