<asp:webpartzone runat="server" CssClass="asp_WPZ" BorderColor="#DDDDDD" Width="100%" Height="100%" DragHighlightColor="#C00000" BorderStyle="Dotted" Padding="5"
    EmptyZoneText="Add a content to this zone by dropping it here, or select from Content Panel." >
    
    <HeaderStyle CssClass="asp_WPZHeader"></HeaderStyle>
    <PartTitleStyle CssClass="asp_WPTitle"></PartTitleStyle>
    <PartChromeStyle CssClass="asp_WPChrome"></PartChromeStyle>
    <MenuPopupStyle BorderStyle="Solid" ShadowColor="#999999" BackColor="#CCDDFF" Font-Names="Tahoma,Arial" Font-Size="8pt" BorderColor="#7E9EC9" BorderWidth="1px"></MenuPopupStyle>
    <MenuVerbHoverStyle BackColor="White"></MenuVerbHoverStyle>
    <MenuLabelHoverStyle BackColor="#CCDDFF" ForeColor="Black"></MenuLabelHoverStyle>
    <MenuVerbStyle ForeColor="Black"></MenuVerbStyle>
    <PartStyle CssClass="asp_WPItem"></PartStyle>
    <EmptyZoneTextStyle CssClass="asp_WPZEmpty" />
</asp:webpartzone>

<asp:catalogzone runat="server" BorderColor="#C04000" BackColor="#FFFFDD" BorderWidth="0px" BorderStyle="Solid" Padding="8" PartChromeType="TitleAndBorder" CssClass="asp_CatZone">
    
    <PartTitleStyle CssClass="asp_CatPartTtl" />
    <FooterStyle CssClass="asp_CatFooter" />
    <CloseVerb Visible="False" />
    <PartChromeStyle BorderStyle="None" BorderWidth="0px" />
    <HeaderStyle CssClass="asp_CatHead" />    
    <PartStyle CssClass="asp_CatPart" />
    <PartLinkStyle CssClass="asp_CatPartLink" />
    <SelectedPartLinkStyle CssClass="asp_CatPartLinkSelected" />
    <InstructionTextStyle CssClass="asp_CatInstrTxt" />
    
</asp:catalogzone>       


<asp:Menu runat="server">
    <StaticMenuStyle CssClass="asp_SMnuSB" />
    <StaticSelectedStyle CssClass="asp_SMnuSelectedSB" BorderStyle="Solid" BorderWidth="1px" BorderColor="#FF9966" BackColor="#FFFFCC" />
    <StaticMenuItemStyle CssClass="asp_SMnuItmSB" Font-Size="60%" BorderStyle="Solid" BorderWidth="1px" BorderColor="#F2F5F7" />
    <StaticHoverStyle CssClass="asp_SMnuHoverSB" ForeColor="#990000" BackColor="#DDEEFF" BorderStyle="Solid" BorderWidth="1px" BorderColor="#316AC5" />    
    <DynamicMenuStyle CssClass="asp_DMnuSB" />
    <DynamicSelectedStyle CssClass="asp_DMnuSelectedSB" BorderStyle="Solid" BorderWidth="1px" BorderColor="#FF9966" BackColor="#FFFFCC" />
    <DynamicMenuItemStyle CssClass="asp_DMnuItmSB" Font-Size="70%" BorderStyle="Solid" BorderWidth="1px" BorderColor="#F2F5F7" />
    <DynamicHoverStyle CssClass="asp_DMnuHoverSB"  ForeColor="#990000" BackColor="#DDEEFF" BorderStyle="Solid" BorderWidth="1px" BorderColor="#316AC5" />      
</asp:Menu>


