    <MenuVerbStyle BorderStyle="Solid" BorderWidth="1px" BorderColor="#507CD1" ForeColor="White"></MenuVerbStyle>
    
    <PartTitleStyle CssClass="asp_WPTtl" />
    <PartChromeStyle CssClass="asp_WPChrome" />
    <PartStyle CssClass="asp_WPItm" />
    
