		</Namespaces>
	</MetaData>
	<Mapping>
		<Element>
			<Name>/enh:Document/enh:Header</Name>
		</Element>
		<SQL>
			SELECT CONVERT(CHAR(10), GETDATE(), 120) + 'T' + CONVERT(CHAR(8), GETDATE(), 108) as GEN_DATE
		</SQL>
		<Map>
			<Element>enh:CreationTime</Element>
			<Value>GEN_DATE</Value>
		</Map>
	</Mapping>
	<Mapping mandatory="false" remove="/enh:Document/enh:Payload[@Operation = 'FacilityAddUpdate']/od:OpenDumpFacilitySiteList/od:OpenDumpFacilitySiteAllDetails">

		<Element>
			<Name>/enh:Document/enh:Payload[@Operation = 'FacilityAddUpdate']/od:OpenDumpFacilitySiteList/od:OpenDumpFacilitySiteAllDetails</Name>
		</Element>
		<SQL keys="STATE_FACILITY_IDENTIFIER,STATE_FACILITY_SYSTEM_ACRONYM_NAME">
      select top 200 * from (select top 201 * from (select distinct top 201 
      A.STATE_FACILITY_IDENTIFIER , A.STATE_FACILITY_SYSTEM_ACRONYM_NAME, A.ORGANIZATION_IDENTIFIER, A.ORGANIZATION_IDENTIFIER_CONTEXT
      , A.SITE_CERTIFIED_OPERATOR_INDICATOR, A.INDIAN_POPULATION_MEASURE, A.NON_INDIAN_POPULATION_MEASURE, A.SOLID_WASTE_SYSTEM_TYPE_NAME
      , A.SITE_STATUS_TEXT, A.SITE_CONDITION_TEXT, A.SITE_CONDITION_DATE, A.FDS_INDENTIFIER, A.TRIBAL_LAND_STATUS_TEXT
      , A.SITE_SURFACE_AREA_MEASUREMENT, A.SITE_SURFACE_VOLUME_MEASUREMENT, A.TRIBAL_NAME, A.TRIBAL_CODE, A.SITE_COMMUNITY_NAME
      , substring(A.SITE_COMMUNITY_CODE,1,10) AS SITE_COMMUNITY_CODE, A.TRIBAL_LAND_NAME AS TRIBAL_LAND_NAME1
      , substring(A.SITE_RESERVATION_CODE,1,4) AS SITE_RESERVATION_CODE, A.IHS_SERVICE_UNIT_NAME, A.IHS_SERVICE_UNIT_CODE
      , A.IHS_DISTRICT_NAME, A.IHS_DISTRICT_CODE, A.IHS_FIELD_OFFICE_NAME, A.IHS_FIELD_OFFICE_CODE, A.EPA_REGION_CODE 	,A.SITE_HEALTH_THREAT_SCORE_VALUE, A.SITE_HEALTH_THREAT_SCORE_TEXT, A.SITE_EPA_THREAT_CONCURRENCE_INDICATOR
      , A.SITE_HEALTH_THREAT_COMMENT_TEXT, A.SITE_RAINFALL_TEXT
      , substring(A.SITE_DRAINAGE_LEACHATE_TEXT,1,65) as SITE_DRAINAGE_LEACHATE_TEXT
      , substring(A.SITE_FLOODING_POTENTIAL_TEXT,1,30) AS SITE_FLOODING_POTENTIAL_TEXT
      , A.SITE_COVER_FREQUENCY_TEXT, A.SITE_BURNING_FREQUENCY_TEXT
      , case when A.SITE_FENCED_INDICATOR = 'No' then 'N' when A.SITE_FENCED_INDICATOR = 'Yes' then 'Y' end as SITE_FENCED_INDICATOR
      , A.SITE_CONTROLLED_ACCESS_TEXT
      , A.SITE_PUBLIC_CONCERN_TEXT, A.SITE_VERTICAL_DISTANCE_TO_AQUIFER, A.SITE_HORIZONTAL_DISTANCE_TO_SURFACE_WATER_TEXT
      , A.SITE_DISTANCE_TO_HOMES_TEXT

      , convert(char(10),	C.LAST_REPORTED_DATE, 120)  AS LAST_REPORT
      , C.SOURCE_OF_DATA, C.FACILITY_REGISTRY_IDENTIFIER
      , C.FACILITY_SITE_NAME, C.FEDERAL_FACILITY_INDICATOR, C.TRIBAL_LAND_INDICATOR, C.TRIBAL_LAND_NAME AS TRIBAL_LAND_NAME2
      , C.CONGRESSIONAL_DISTRICT_NUMBER, C.LEGISLATIVE_DISTRICT_NUMBER, C.LOCATION_ADDRESS_TEXT
      , C.SUPPLEMENTAL_LOCATION_TEXT, C.LOCALITY_NAME, C.COUNTY_STATE_FIPS_CODE, C.COUNTY_NAME
      , C.STATE_USPS_CODE, C.STATE_NAME, C.COUNTRY_NAME, C.LOCATION_ZIP_CODE, C.LOCATION_DESCRIPTION_TEXT
      , C.MAILING_ADDRESS_TEXT, C.SUPPLEMENTAL_ADDRESS_TEXT, C.MAILING_ADDRESS_CITY_NAME
      , C.MAILING_ADDRESS_STATE_USPS_CODE, C.MAILING_ADDRESS_STATE_NAME, C.MAILING_ADDRESS_COUNTRY_NAME
      , C.MAILING_ADDRESS_ZIP_CODE

      FROM OD_OPEN_DUMP_DETAILS A, FRS_ENVIRONMENTAL_INTEREST B, FRS_FACILITY_SITE_DATA C, FRS_GEOGRAPHIC_DETAILS D
      WHERE A.STATE_FACILITY_IDENTIFIER = B.STATE_FACILITY_IDENTIFIER
      AND A.STATE_FACILITY_SYSTEM_ACRONYM_NAME = B.STATE_FACILITY_SYSTEM_ACRONYM_NAME
      AND A.STATE_FACILITY_IDENTIFIER = C.STATE_FACILITY_IDENTIFIER
      AND A.STATE_FACILITY_SYSTEM_ACRONYM_NAME = C.STATE_FACILITY_SYSTEM_ACRONYM_NAME
      AND B.ENVIRONMENTAL_INTEREST_TYPE_TEXT = 'OpenDump'
      AND C.DELETED_INDICATOR &lt;&gt; 'Y'
      AND C.LAST_REPORTED_DATE >= convert(datetime, '@LastReportedDate@', 101)
      AND C.FACILITY_SITE_NAME = '@FacilitySiteName@'
      AND CAST(D.LATITUDE_MEASURE AS NUMERIC(15, 8)) BETWEEN 1.0 AND 2.0
      AND CAST(D.LONGITUDE_MEASURE AS NUMERIC(15, 8)) BETWEEN -95 AND -94
      AND C.COUNTY_NAME = 'COUNTY'
      AND A.SITE_HEALTH_THREAT_SCORE_TEXT = '@HealthThreatScore@' 
      order by STATE_FACILITY_IDENTIFIER asc) tempresult order by STATE_FACILITY_IDENTIFIER desc) finalresult
    </SQL>

		<Conditional>
