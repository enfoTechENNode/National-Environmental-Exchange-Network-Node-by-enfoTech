<?xml version="1.0" encoding="UTF-8"?>
<MappingScript>
	<DataSource type="System.Data.SqlClient">
		<Value>Data Source=ROME;Initial Catalog=CN_FRS;User ID=CN_FRS;password=CN_FRS</Value>
	</DataSource>
	<MetaData>
		<Namespaces>
			<NameValues>
				<Name>enh</Name>
				<Value>http://www.exchangenetwork.net/schema/v1.0/ExchangeNetworkDocument.xsd</Value>
			</NameValues>
			<NameValues>
				<Name>od</Name>
				<Value>http://www.exchangenetwork.net/schema/od/1</Value>
			</NameValues>
			<NameValues>
				<Name>facid</Name>
				<Value>http://www.epa.gov/xml</Value>
			</NameValues>
			<NameValues>
				<Name>sc</Name>
				<Value>urn:us:net:exchangenetwork:sc:1:0</Value>
