				<Name>facid:IndividualDetails</Name>
			</Element>
			<SQL>
				select distinct AFFILIATION_TYPE_TEXT, convert(char(10), AFFILIATION_START_DATE, 120) AFF_START_DATE, convert(char(10), AFFILIATION_END_DATE, 120) AFF_END_DATE, EMAIL_ADDRESS_TEXT, TELEPHONE_NUMBER, PHONE_EXTENSION, FAX_NUMBER, ALTERNATE_TELEPHONE_NUMBER, INDIVIDUAL_FULL_NAME, INDIVIDUAL_TITLE_TEXT from FRS_INDIVIDUAL_DETAILS where STATE_FACILITY_IDENTIFIER = '@STATE_FACILITY_IDENTIFIER@' and STATE_FACILITY_SYSTEM_ACRONYM_NAME = '@STATE_FACILITY_SYSTEM_ACRONYM_NAME@'
			</SQL>
			<Conditional>
				<if>
					<Column>AFFILIATION_TYPE_TEXT</Column>
